{
  "name": "@liip/postcss-class-prefixer",
  "version": "0.0.1",
  "description": "PostCSS plugin to automatically prefix and contain CSS class names",
  "main": "dist/plugin.js",
  "keywords": [
    "css",
    "prefix",
    "class",
    "postcss",
    "plugin"
  ],
  "author": {
    "name": "Liip",
    "url": "https://www.liip.ch/"
  },
  "license": "MIT",
  "scripts": {
    "dev": "tsc -w -p ./tsconfig.build.json",
    "build": "tsc -p ./tsconfig.build.json",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier '**/!(dist)/*.@(ts|json|md)' --check",
    "format:fix": "npm run format -- --write",
    "test": "NODE_ENV=test jest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/clean-css": "^4.2.6",
    "@types/css-selector-tokenizer": "^0.7.1",
    "clean-css": "^5.3.2",
    "postcss": "^8.4.24",
    "postcss-nested": "^6.0.1"
  },
  "dependencies": {
    "@liip/class-prefixer-core": "^0.0.1",
    "css-selector-tokenizer": "^0.8.0"
  },
  "peerDependencies": {
    "postcss": "> 8"
  }
}
