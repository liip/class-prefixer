{
  "name": "@liip/esbuild-plugin-ast",
  "version": "0.1.0-alpha.0",
  "description": "Esbuild plugin to parse JavaScript AST",
  "keywords": [
    "esbuild",
    "plugin",
    "ast"
  ],
  "author": {
    "name": "Liip",
    "url": "https://www.liip.ch/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liip/class-prefixer.git",
    "directory": "packages/esbuild-plugin-ast"
  },
  "publishConfig": {
    "@liip:registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "main": "dist/plugin.js",
  "files": [
    "/dist"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "tsc -w -p ./tsconfig.build.json",
    "build": "tsc -p ./tsconfig.build.json",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier '**/!(dist)/*.@(ts|json|md)' --check",
    "format:fix": "npm run format -- --write",
    "test": "NODE_ENV=test jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@liip/class-prefixer-core": "^0.1.0-alpha.0",
    "acorn": "^8.9.0",
    "astring": "^1.8.6",
    "estraverse": "^5.3.0"
  },
  "devDependencies": {
    "@types/estraverse": "^5.1.2",
    "esbuild": "^0.18.6",
    "prettier": "^2.8.8"
  },
  "peerDependencies": {
    "esbuild": ">= 0.18"
  }
}
